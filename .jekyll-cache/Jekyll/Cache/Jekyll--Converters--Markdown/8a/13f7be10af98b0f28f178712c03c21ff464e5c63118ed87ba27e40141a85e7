<mtext mathvariant="bold">g</mtext><mn>3</mn></msub></mrow></mstyle></mtd><mtd><mstyle scriptlevel="0" displaystyle="true"><mrow><mrow></mrow><mo>=</mo><msup><mrow><mo fence="true">(</mo><msubsup><mi>g</mi><mn>1</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>5</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>3</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>2</mn></msub></mrow></msubsup><msubsup><mi>g</mi><mn>7</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msub><mi>x</mi><mn>2</mn></msub></mrow></msubsup><mo fence="true">)</mo></mrow><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></msup><mo>âˆ˜</mo><msup><mrow><mo fence="true">(</mo><msubsup><mi>g</mi><mn>2</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>6</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>4</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>2</mn></msub></mrow></msubsup><msubsup><mi>g</mi><mn>8</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msub><mi>x</mi><mn>2</mn></msub></mrow></msubsup><mo fence="true">)</mo></mrow><msub><mi>x</mi><mn>3</mn></msub></msup></mrow></mstyle></mtd></mtr><mtr><mtd><mstyle scriptlevel="0" displaystyle="true"><mrow></mrow></mstyle></mtd><mtd><mstyle scriptlevel="0" displaystyle="true"><mrow><mrow></mrow><mo>=</mo><msubsup><mi>g</mi><mn>1</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>5</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>3</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>2</mn></msub><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>7</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msub><mi>x</mi><mn>2</mn></msub><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>2</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>3</mn></msub></mrow></msubsup><msubsup><mi>g</mi><mn>6</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>3</mn></msub></mrow></msubsup><msubsup><mi>g</mi><mn>4</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msub><mi>x</mi><mn>2</mn></msub><msub><mi>x</mi><mn>3</mn></msub></mrow></msubsup><msubsup><mi>g</mi><mn>8</mn><mrow><msub><mi>x</mi><mn>1</mn></msub><msub><mi>x</mi><mn>2</mn></msub><msub><mi>x</mi><mn>3</mn></msub></mrow></msubsup></mrow></mstyle></mtd></mtr><mtr><mtd><mstyle scriptlevel="0" displaystyle="true"><mrow></mrow></mstyle></mtd><mtd><mstyle scriptlevel="0" displaystyle="true"><mrow><mrow></mrow><mo>=</mo><msubsup><mi>g</mi><mn>1</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>2</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mn>1</mn></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>3</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>4</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>2</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>3</mn><mn>1</mn></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>5</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>6</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>2</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup><msubsup><mi>x</mi><mn>3</mn><mn>1</mn></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>7</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>2</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>3</mn><mrow><mo>âˆ’</mo><mn>1</mn></mrow></msubsup></mrow></msubsup><msubsup><mi>g</mi><mn>8</mn><mrow><msubsup><mi>x</mi><mn>1</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>2</mn><mn>1</mn></msubsup><msubsup><mi>x</mi><mn>3</mn><mn>1</mn></msubsup></mrow></msubsup><mo>âˆˆ</mo><mi mathvariant="double-struck">G</mi></mrow></mstyle></mtd></mtr></mtable><annotation encoding="application/x-tex">
\begin{aligned}
\textbf{g}_1 &amp;= \textcolor{red}{\textbf{g}}_{1}^{x_1^{-1}} \circ \textcolor{blue}{\textbf{g}}_{1}^{x_1} \in \mathbb{G}^{\frac{n}{2}}
\\
             &amp;= (g_1, g_2, g_3, g_4)^{x_1^{-1}} \circ (g_5, g_6, g_7, g_8)^{x_1}
\\
             &amp;= (g_1^{x_1^{-1}} g_5^{x_1}, \  g_2^{x_1^{-1}} g_6^{x_1}, \  g_3^{x_1^{-1}} g_7^{x_1}, \  g_4^{x_1^{-1}} g_8^{x_1})
             \in \mathbb{G}^4
\\
\textbf{g}_2 &amp;= \left( g_1^{x_1^{-1}} g_5^{x_1}, \  g_2^{x_1^{-1}} g_6^{x_1} \right)^{x_2^{-1}} \circ
                \left( g_3^{x_1^{-1}} g_7^{x_1}, \  g_4^{x_1^{-1}} g_8^{x_1} \right)^{x_2} 
\\
             &amp;= \left( 
                 g_1^{x_1^{-1} x_2^{-1}} g_5^{x_1 x_2^{-1}} g_3^{x_1^{-1} x_2} g_7^{x_1 x_2}, \ 
                 g_2^{x_1^{-1} x_2^{-1}} g_6^{x_1 x_2^{-1}} g_4^{x_1^{-1} x_2} g_8^{x_1 x_2}
                \right)
                \in \mathbb{G}^2
\\
g_\text{last} = \textbf{g}_3 &amp;= 
                \left( g_1^{x_1^{-1} x_2^{-1}} g_5^{x_1 x_2^{-1}} g_3^{x_1^{-1} x_2} g_7^{x_1 x_2} \right)^{x_3^{-1}} \circ
                \left( g_2^{x_1^{-1} x_2^{-1}} g_6^{x_1 x_2^{-1}} g_4^{x_1^{-1} x_2} g_8^{x_1 x_2} \right)^{x_3}
\\
                &amp;= g_1^{x_1^{-1} x_2^{-1} x_3^{-1}} g_5^{x_1 x_2^{-1} x_3^{-1}} g_3^{x_1^{-1} x_2 x_3^{-1}} g_7^{x_1 x_2 x_3^{-1}}
                   g_2^{x_1^{-1} x_2^{-1} x_3} g_6^{x_1 x_2^{-1} x_3} g_4^{x_1^{-1} x_2 x_3} g_8^{x_1 x_2 x_3}
\\
                &amp;= g_1^{x_1^{-1} x_2^{-1} x_3^{-1}} g_2^{x_1^{-1} x_2^{-1} x_3^{1}} g_3^{x_1^{-1} x_2^{1} x_3^{-1}} g_4^{x_1^{-1} x_2^{1} x_3^{1}}
                   g_5^{x_1^{1} x_2^{-1} x_3^{-1}} g_6^{x_1^{1} x_2^{-1} x_3^{1}} g_7^{x_1^{1} x_2^{1} x_3^{-1}} g_8^{x_1^{1} x_2^{1} x_3^{1}}
                   \in \mathbb{G}
\end{aligned}
